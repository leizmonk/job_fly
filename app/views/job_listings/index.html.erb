<h1>Saved Jobs</h1>

<ul class="job-listing-info">
<% @job_listing.each do |listing| %>
	<li><%= listing.position_name %></li>
	<li><%= listing.company_name %> - <%= listing.location %></li>
	<li><%= listing.description %></li>
	<li><%= listing.responsibilities %></li>
	<li><%= listing.qualifications %></li>
	<li><%= listing.application_requirements %></li>
	<li><%= listing.status %></li>
	<li><%= listing.listing_url %></li>
	<li><%= listing.date_posted %></li>
	<li><%= listing.date_closing %></li>
<% end %>
</ul>

